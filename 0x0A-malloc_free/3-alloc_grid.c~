#include <stdlib.h>
#include <stdio.h>
#include "holberton.h"
/**
 * alloc_grid - returns a pointer to a 2 dimensional array
 *
 * @width: width of grid
 * @height: height of grid
 *
 * Return: pointer
 *
 */

int **alloc_grid(int width, int height)
{
	int x, y;
	int *m;

	 m = (int *)malloc (height * sizeof(int));
	 for (y = 0, y < height, y++);
	 {
		 m = (int *)malloc (width * sizeof(int));
		 for(x = 0, x < width; x++)
		 {
			 m[x][y] = 0;
		 }
	 }

