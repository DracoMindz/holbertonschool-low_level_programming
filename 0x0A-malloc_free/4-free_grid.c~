#include <stdio.h>
#include <stdlib.h>
#include "holberton.h"

/**
 * create_array - program creates an array of chars, intializes with char
 * @size: size of int
 * @c: variable
 *
 * Description: function creates array of chars initializes with specific char
 *
 * Return: 0
 */
void free_grid(int **grid, int height)
{
	for (y = 0; y < height; y++)
	{
		free grid[y];
	}
	free[grid];
}
