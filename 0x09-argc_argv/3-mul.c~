#include <stdio.h>
#include "holberton.h"

/**
 * main - program multiplies two numbers
 * @argc: count number of arguements
 * @argv: pointer
 *
 * Description: program multiplies two numbers
 *
 * Return: 0
 */
int main(int argc, char *argv[])
{
	int num1, num2, result;

	if (argc != 3 || argc == 0)
	{
		printf("Error");
		return (1);
	}
	num1 = atoi(argv[1]);
	num2 = atoi(argv[2]);
	result = num1 * num2;
	printf("%d\n", result);
	return (0);
}
